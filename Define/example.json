{
    "example": {
        "DCC": "on",
        "Host": "IP|Hostname",
        "HostGroup": "web",
        "Template": {
            "src": "example.json",
            "dst": "/etc/init.d/example",
            "mode": 644,
            "owner": "root",
            "group": "root",
            "backup": "on"
        },
        "Define": {
            "Command": "example",
            "start": "start",
            "stop": "stop",
            "reload": "reload",
            "restart": "restart"
        }
    }
}
